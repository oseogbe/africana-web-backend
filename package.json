{
  "name": "africana-backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "start": "node -r tsconfig-paths/register src/app.js",
    "dev": "ts-node -r tsconfig-paths/register src/app.ts",
    "dev:watch": "nodemon src/app.ts -e ts,json --exec 'npm run dev'",
    "migrate": "prisma migrate dev --name",
    "prisma:migrate": "prisma migrate dev --skip-seed",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "Osememen Ogbe",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.8",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.8",
    "@types/nodemailer": "^6.4.14",
    "@types/winston": "^2.4.4",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.8.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
